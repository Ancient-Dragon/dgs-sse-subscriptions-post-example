{
  "name": "vue-urql-nexus",
  "version": "0.0.0",
  "scripts": {
    "server": "ts-node graphql/server.ts",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "codegen": "graphql-codegen --config ${PWD}/graphql-codegen.yml",
    "build-schema": "ts-node --transpile-only graphql/schema.ts"
  },
  "dependencies": {
    "@urql/vue": "^1.0.4",
    "@vueuse/core": "^9.13.0",
    "graphql-sse": "^2.1.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typed-document-node": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.1.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "nexus": "^1.3.0",
    "nexus-decorators": "^0.2.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
